
<div id="page-wrapper">
<div class="container" >
    <div class="row" >
        <div class="col-md-6">
            <a href="/admin/addCategory" class="btn btn-primary " style="margin-top: 20px;">Add New Category</a>
        </div>
    </div>
    <table class="table table-striped mt-5" style="margin-top:30px;" id="userTable">
  <thead>
    <tr>
        <th scope="col">No.</th>
      <th scope="col">Name</th>
      <th scope="col">Delete</th>
      
      
      
    </tr>
  </thead>
  <tbody>
      {{#each category}}
    <tr>

      <td>{{inc @index}}</td>
      <td>{{this.Category}}</td>
      
      
     
     
      <td><a href ="/admin/deleteCategory?id={{this._id}}" class="btn btn-danger" onclick="return confirm('Do you want to Delete {{this.Category}}')">Delete</a> </td> 
    </tr>
    {{/each}}
   
  </tbody>
</table>
</div>
</div>

